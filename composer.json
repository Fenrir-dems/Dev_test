{
    "name": "rappasoft/laravel-5-boilerplate",
    "description": "The Laravel Boilerplate Project.",
    "keywords": ["framework", "laravel", "boilerplate"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "appstract/laravel-blade-directives": "^0.4.4",
        "arcanedev/log-viewer": "^8.1.0",
        "arcanedev/no-captcha": "12.*",
        "bensampo/laravel-enum": "^3.4.1",
        "creativeorange/gravatar": "~1.0",
        "cviebrock/eloquent-taggable": "^8.0",
        "davejamesmiller/laravel-breadcrumbs": "5.3.2",
        "dg/rss-php": "^1.4",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hieu-le/active": "^8.0.0",
        "kalnoy/nestedset": "^6.0.0",
        "laravel/framework": "^8.40",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "phpoffice/phpspreadsheet": "^1.5",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-html": "^2.4",
        "spatie/laravel-permission": "^3.0",
        "webpatser/laravel-uuid": "^4.0",
        "wildside/userstamps": "^2.1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.15.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "barryvdh/laravel-debugbar": "^3.3.0",
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "^6.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "clear-all": [
            "composer dumpautoload -o",
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear"
        ],
        "phpunit": [
            "phpunit > output.txt -d memory_limit=2048M"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
